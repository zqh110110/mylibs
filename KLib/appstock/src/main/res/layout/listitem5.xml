<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:cacheColorHint="#000000"
    android:descendantFocusability="blocksDescendants"
    android:orientation="horizontal" 
     android:paddingTop="5dp"
    android:paddingBottom="5dp"
    android:paddingLeft="5dp">


  

    <RelativeLayout
        android:id="@+id/leftlayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true" >
            <TextView
        android:id="@+id/bianmaname"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:singleLine="true"
        android:text="@string/wait"
            android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black" />
             <TextView
        android:id="@+id/stockcode"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:singleLine="true"
        android:text="@string/wait"
        android:layout_below="@+id/bianmaname"
            android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black" />
    </RelativeLayout>
    
    <com.kfd.ui.InterceptScrollContainer
        android:id="@+id/scroollContainter"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/leftlayout"
        android:focusable="false" >

        <com.kfd.ui.MyHScrollView
            android:id="@+id/horizontalScrollView1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:focusable="false"
            android:scrollbars="none" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:focusable="false"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/jianchangtime"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                 
                    android:text="@string/wait"
                         android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black" />


                <TextView
                    android:id="@+id/type"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="@string/wait"
                         android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black"/>

                <TextView
                    android:id="@+id/jianchangjia"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="@string/wait"
                         android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black" />

                <TextView
                    android:id="@+id/downjia"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="@string/wait"
                          android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black" />

                <TextView
                    android:id="@+id/upjia"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="@string/wait"
                        android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black" />
                   <TextView
                    android:id="@+id/nowjia"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="@string/wait"
                        android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black" />
                   <TextView
                    android:id="@+id/count"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="@string/wait"
                         android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black"/>
                   <TextView
                    android:id="@+id/jianchangfei"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="@string/wait"
                         android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black" />
                   <TextView
                    android:id="@+id/hotfei"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="@string/wait"
                          android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black" />
                   <TextView
                    android:id="@+id/updown"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:singleLine="true"
                    android:visibility="gone"
                    android:text="@string/wait"
                          android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black" />
                   <TextView
                    android:id="@+id/daycount"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="@string/wait"
                         android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black" />
                   <TextView
                    android:id="@+id/liuchangfei"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="@string/wait"
                         android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black"/>
                   <TextView
                    android:id="@+id/fudongyin"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="@string/wait"
                        android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black"/>
                <TextView
                    android:id="@+id/bills"
                    android:layout_width="160dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:singleLine="true"
                    android:visibility="gone"
                    android:text="@string/wait"
                          android:textSize="@dimen/text_size_15"
                    android:textColor="@color/listitem_black" />
 
            </LinearLayout>
        </com.kfd.ui.MyHScrollView>
    </com.kfd.ui.InterceptScrollContainer>

</RelativeLayout>